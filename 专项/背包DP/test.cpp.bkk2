#include <algorithm>
#include <cstdio>
#include <iostream>
#include <utility>

using namespace std;
// typedef long long ll;
const int N = 1e4+10, D = 30;
int q[N][D],K_[N][D],v[N][D],w[N];
int res_qk[N][N];
long long res[N][D];
int n,d;

// void debug_res_qk(){
